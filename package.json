    "start:workflow-builder:dev": "echo \"Error: no test specified\" && exit 1",
    "start:web-ui:dev": "echo \"Error: no test specified\" && exit 1"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/**/*"
      ],
      "production": [
        "!{projectRoot}/**/*.spec.tsx"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "production",
          "^production"
        ],
        "outputs": [
          "dist/libs/mylib"
        ],
        "dependsOn": [
          "^build"
        ]
      },
      "test": {
        "inputs": [
          "default",
          "^production"
        ],
        "outputs": [],
        "dependsOn": [
          "build"
        ]
      }
    }
  },
